<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Master.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/Color.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.4.4.min.js")" type="text/javascript"></script>
</head>

<body>
    <div id="cabecalho">
        <img alt="Biologia" class="logo" src="@Url.Content("~/Content/images/Bio.png")" />
        <img alt="Semana Acadêmica de Biologia" src="@Url.Content("~/Content/images/Titulo.png")" />
        @Html.Partial("_LogOnPartial")
    </div>
    <div id="main">
        @{Html.RenderAction("MenuSuperior", "Menu");}
        <div id="insideMain">
            @RenderBody()
        </div>
    </div>
    <div id="base">
    </div>
</body>
</html>
